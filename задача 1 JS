<script>
var sum=0,s=0
// To start we will create 2 variables:
var matrixExample = [
[ 1, 2, 3, 4 ],
[ 4, 5, 6, 5 ],
[ 7, 8, 9, 7 ],
[ 7, 8, 9, 7 ]
];
function sumUpDiagonals(matrixExample) {
for (var i=0; i<4; i=i+1) {
sum=sum+matrixExample[i][i]
s += matrixExample[i][4-i-1]
}
alert(sum+s)
}
console.log(sumUpDiagonals(matrixExample))
</script>
